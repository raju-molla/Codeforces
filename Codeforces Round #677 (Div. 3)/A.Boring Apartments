#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string s=to_string(n);
        int l=s.size();
        if(s[0]=='1'){
            if(l==1){
                cout<< "1"<<endl;
            }
            else if(l==2){
                cout<< "3"<<endl;
            }
            else if(l==3){
                cout<< "6"<<endl;
            }
            else if(l==4)cout<< "10"<<endl;
        }
       else if(s[0]=='2'){
            if(l==1){
                cout<< 10+1<<endl;
            }
            else if(l==2){
                cout<< 10+3<<endl;
            }
            else if(l==3){
                cout<< 10+6<<endl;
            }
            else if(l==4)cout<< 10+10<<endl;
        }
       else if(s[0]=='3'){
            if(l==1){
                cout<< 20+1<<endl;
            }
            else if(l==2){
                cout<< 20+3<<endl;
            }
            else if(l==3){
                cout<< 20+6<<endl;
            }
            else if(l==4)cout<< 20+10<<endl;
        }
        else if(s[0]=='4'){
            if(l==1){
                cout<< 30+1<<endl;
            }
            else if(l==2){
                cout<< 30+3<<endl;
            }
            else if(l==3){
                cout<< 30+6<<endl;
            }
            else if(l==4)cout<< 30+10<<endl;
        }
        else if(s[0]=='5'){
            if(l==1){
                cout<< 40+1<<endl;
            }
            else if(l==2){
                cout<< 40+3<<endl;
            }
            else if(l==3){
                cout<< 40+6<<endl;
            }
            else if(l==4)cout<< 40+10<<endl;
        }
        else if(s[0]=='6'){
            if(l==1){
                cout<< 50+1<<endl;
            }
            else if(l==2){
                cout<< 50+3<<endl;
            }
            else if(l==3){
                cout<< 50+6<<endl;
            }
            else if(l==4)cout<< 50+10<<endl;
        }
        else if(s[0]=='7'){
            if(l==1){
                cout<< 60+1<<endl;
            }
            else if(l==2){
                cout<< 60+3<<endl;
            }
            else if(l==3){
                cout<< 60+6<<endl;
            }
            else if(l==4)cout<< 60+10<<endl;
        }
        else if(s[0]=='8'){
            if(l==1){
                cout<< 70+1<<endl;
            }
            else if(l==2){
                cout<< 70+3<<endl;
            }
            else if(l==3){
                cout<< 70+6<<endl;
            }
            else if(l==4)cout<< 70+10<<endl;
        }
        else if(s[0]=='9'){
            if(l==1){
                cout<< 80+1<<endl;
            }
            else if(l==2){
                cout<< 80+3<<endl;
            }
            else if(l==3){
                cout<< 80+6<<endl;
            }
            else if(l==4)cout<< 80+10<<endl;
        }

    }
}
