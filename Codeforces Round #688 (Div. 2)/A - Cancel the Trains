#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define Fast ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
#define ForN(i,a,b) for(int i=a; i<b; i++)
#define ForE(i,a,b) for(int i=a; i<=b; i++)

int main()
{

    Fast
    int t;
    cin>>t;
    while(t--){
    int n,m;
    cin>>n>>m;
    int a[n],b[m];
    for(int i=0; i<n; i++){
        cin>>a[i];
    }
    for(int i=0; i<m; i++){
        cin>>b[i];
    }
    int c=0;
    for(int i=0; i<n; i++){
        for(int j=0; j<m; j++){
            if(a[i]==b[j]){
                c++;
            }
        }
    }
    cout<<c<<endl;
    }

}
