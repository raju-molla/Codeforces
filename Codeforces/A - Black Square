#include<bits/stdc++.h>
using namespace std;

int main()
{

    vector<int>v(4);
    for(int i=1; i<=4; i++){
        cin>>v[i];
    }
    string s;
    cin>>s;
    int sum=0;
    for(int i=0; i<s.size(); i++){
        char x=s[i];
        int p=x;
        p=p-48;
        sum+=v[p];
    }
    cout<<sum<<endl;
}
