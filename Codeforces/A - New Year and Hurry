#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n,k;
    cin>>n>>k;
    int m=240-k;
    int c=0;
    int s=0;
    int i=5;
    while(m>=s){
        s+=i;
        if(m>=s){
            c++;
        }
        i+=5;
    }
    if(c>n){
        cout<<n<<endl;
        return 0;
    }
    cout<<c<<endl;


}
