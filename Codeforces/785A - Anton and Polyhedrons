#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int n;
    cin>>n;
    vector<int>v;
    for(int i=0; i<n; i++)
    {
        string s;
        cin>>s;
        if(s=="Tetrahedron"){
            v.push_back(4);
        }
        else if(s=="Cube"){
            v.push_back(6);
        }
        else if(s=="Octahedron"){
            v.push_back(8);
        }
        else if(s=="Dodecahedron"){
            v.push_back(12);
        }
        else if(s=="Icosahedron"){
            v.push_back(20);
        }
 
    }
    int sum=0;
    for(int i=0; i<v.size(); i++)
    {
        sum+=v[i];
    }
    cout<<sum<<endl;
}
 
