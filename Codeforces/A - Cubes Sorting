#include<bits/stdc++.h>
using namespace std;


int  main()
{
    int t;
    cin>>t;
    while (t--)
    {
        int n;
        cin>>n;
        int a[n];
        for(int i=0; i<n; i++){
            cin>>a[i];
        }
        int ok=false;
        for(int i=1; i<n; i++){
            if(a[i]>=a[i-1]){
                ok=true;
                break;
            }
        }
        if(ok)cout<< "YES\n";
        else cout<< "NO\n";
    }
    
}
