#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int n;
    cin>>n;
    if(n<=2){
        cout<<n<<endl;
    }
    else if(n==3)cout<< "6"<<endl;
    else if(n==4)cout<< "20"<<endl;
    else if(n==5)cout<< "70"<<endl;
    else if(n==6)cout<< "252"<<endl;
    else if(n==7)cout<< "924"<<endl;
    else if(n==8)cout<< "3432"<<endl;
    else if(n==9)cout<< "12870"<<endl;
    else if(n==10)cout<< "48620"<<endl;
}

/*The another solution 

#include<bits/stdc++.h>
using namespace std;
int a[10][10];
int main()
{
    int n;
    cin>>n;
   for(int i=0; i<n; i++){
       for(int j=0; j<n; j++){
           if(i==0)a[i][j]=1;
           else if(j==0)a[i][j]=1;
           else a[i][j]=a[i-1][j]+a[i][j-1];    
       }
   }
   cout<<a[n-1][n-1];
}

