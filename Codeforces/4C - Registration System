#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define Sort(v) sort(v.begin(),v.end())
#define pb(v) push_back(v)
#define For(strt,n) for(int i=strt; i<n; i++)
int main()
{
    int n;
    cin>>n;
    vector<string>s(n);
    map<string, int>mp;
    for(int i=0; i<n; i++){
        cin>>s[i];
    }
    for(int i=0; i<n; i++){
        mp[s[i]]++;
        int x=mp[s[i]];
        if(x==1){
            cout<< "OK"<<endl;
        }else cout<< s[i]<<x-1<<endl;
    }



}
