#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define Fast ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);

int main()
{

    Fast
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string a,b;
        cin>>a>>b;
        int A=0,B=0,C=0;
        for(int i=0; i<n; i++){
            if(a[i]<b[i]){
                B++;
            }else if(a[i]>b[i]){
                A++;
            }

        }
        if(A>B){
            cout<< "RED"<<endl;
        }
        else if(B>A)cout<< "BLUE"<<endl;
       else cout<< "EQUAL"<<endl;
    }
}





