#include<bits/stdc++.h>
using namespace std;
 
int arr[]= {1,10,100,1000,10000,100000,1000000,10000000,100000000,1000000000};
 
int main(){
 
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string s = to_string(n);
        long long int ans= arr[s.size()-1]-int(n);
        cout<<abs(ans)<<endl;
 
    }
}
