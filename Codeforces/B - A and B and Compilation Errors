#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int x=0,y=0,z=0;
    for(int i=0; i<n; i++){
        int a;
        cin>>a;
        x+=a;
    }

    for(int i=0; i<n-1; i++){
       int a;
       cin>>a;
       y+=a;
    }
    for(int i=0; i<n-2; i++){
        int a;
        cin>>a;
        z+=a;
    }
    cout<<x-y<<endl<<y-z<<endl;

}
