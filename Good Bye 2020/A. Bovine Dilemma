#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define Fast ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);

int main()
{

     Fast
     int t;
     cin>>t;
     while(t--){
        int n;
        cin>>n;
        int arr[n+1];
        for(int i=0; i<n; i++){
            cin>>arr[i];
        }
        set<double>st;
        int ans=0;
        for(int i=0; i<n-1; i++){
            for(int j=i+1; j<n; j++){
                double p=(double)abs(arr[i]-arr[j])/(double)2;
                st.insert(p);
            }
        }
        cout<<st.size()<<endl;
     }

}




