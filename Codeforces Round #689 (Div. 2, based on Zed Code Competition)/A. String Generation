#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define Fast ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);

int main()
{

    Fast
    int t;
    cin>>t;
    while(t--)
    {
        int n,m;
        cin>>n>>m;
        for(int i=0; i<n; i++){
            if(i%3==0){
                cout<< "a";
            }else {
                cout<< (i%3==1? "b": "c");
            }
        }
        cout<<endl;
    }
}
