#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define Fast ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);

int main()
{

    Fast
    int t;
    cin>>t;
    while(t--){
        ll n;
        cin>>n;
        ll a[n+1];
        ll Alic=0,bob=0;
        for(ll i=0; i<n; i++){
            cin>>a[i];
        }
        sort(a,a+n);
        reverse(a,a+n);
        for(ll i=0; i<n; i++){
            if(i%2==0){
                if(a[i]%2==0)Alic+=a[i];
            }else{
                if(a[i]%2)bob+=a[i];
            }
        }
        if(Alic>bob){
            cout<< "Alice"<<endl;
        }
        else if(Alic<bob){
            cout<< "Bob"<<endl;
        }
        else{
            cout<< "Tie"<<endl;
        }
    }

}




